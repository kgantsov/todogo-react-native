// Use this file as a starting point for your project's .eslintrc.
// Copy this file, and add rule overrides as needed.
{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "rules": {
      "jsx-a11y/href-no-hash": "off",
      "react/jsx-filename-extension": [
        1, { "extensions": [".js", ".jsx"] }
      ]
    },
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    }
}
